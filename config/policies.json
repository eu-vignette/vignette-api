{
  "policies": [
    {
      "handler": {
        "export": "ValidateJsonSchemaInbound",
        "module": "$import(@zuplo/runtime)",
        "options": {
          "validator": "$import(./schemas/insert-order.json)"
        }
      },
      "name": "insert-order-policy",
      "policyType": "validate-json-schema-inbound"
    },
    {
      "handler": {
        "export": "ApiKeyInboundPolicy",
        "module": "$import(@zuplo/runtime)",
        "options": {
          "allowUnauthenticatedRequests": false
        }
      },
      "name": "partner-api-key",
      "policyType": "api-key-inbound"
    },
    {
      "handler": {
        "export": "default",
        "module": "$import(./modules/partner-policy-key)"
      },
      "name": "partner-policy-key",
      "policyType": "set-headers-inbound"
    },
    {
      "handler": {
        "export": "ValidateJsonSchemaInbound",
        "module": "$import(@zuplo/runtime)",
        "options": {
          "validator": "$import(./schemas/validate-vehicle-plate.json)"
        }
      },
      "name": "validate-json-schema-inbound",
      "policyType": "validate-json-schema-inbound"
    },
    {
      "handler": {
        "export": "RateLimitInboundPolicy",
        "module": "$import(@zuplo/runtime)",
        "options": {
          "rateLimitBy": "user",
          "requestsAllowed": 2,
          "timeWindowMinutes": 1,
          "identifier": {
            "module": "$import(./modules/rate-limiter)",
            "export": "rateLimitKey"
          }
        }
      },
      "name": "rate-limit-inbound",
      "policyType": "rate-limit-inbound"
    },
    {
      "handler": {
        "export": "RequestValidationInboundPolicy",
        "module": "$import(@zuplo/runtime)",
        "options": {
          "includeRequestInLogs": true,
          "logLevel": "info",
          "validateBody": "reject-and-log",
          "validatePathParameters": "log-only",
          "validateQueryParameters": "log-only"
        }
      },
      "name": "request-validation-inbound",
      "policyType": "request-validation-inbound"
    },
    {
      "handler": {
        "export": "RequestValidationInboundPolicy",
        "module": "$import(@zuplo/runtime)",
        "options": {
          "includeRequestInLogs": false,
          "logLevel": "info",
          "validateBody": "reject-and-log",
          "validatePathParameters": "log-only",
          "validateQueryParameters": "log-only"
        }
      },
      "name": "request-validation-order",
      "policyType": "request-validation-inbound"
    }
  ],
  "corsPolicies": [
    {
      "name": "custom-cors",
      "allowedOrigins": "*",
      "allowedHeaders": "Origin, Content-Type, Accept, Authorization",
      "allowedMethods": ["GET", "POST"],
      "exposeHeaders": "my-special-response-header",
      "maxAge": 600,
      "allowCredentials": true
    }
  ]
}