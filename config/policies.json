{
  "policies": [
    {
      "handler": {
        "export": "ValidateJsonSchemaInbound",
        "module": "$import(@zuplo/runtime)",
        "options": {
          "validator": "$import(./schemas/insert-order.json)"
        }
      },
      "name": "insert-order-policy",
      "policyType": "validate-json-schema-inbound"
    },
    {
      "handler": {
        "export": "ApiKeyInboundPolicy",
        "module": "$import(@zuplo/runtime)",
        "options": {
          "allowUnauthenticatedRequests": false
        }
      },
      "name": "partner-api-key",
      "policyType": "api-key-inbound"
    },
    {
      "handler": {
        "export": "default",
        "module": "$import(./modules/partner-policy-key)"
      },
      "name": "partner-policy-key",
      "policyType": "set-headers-inbound"
    },
    {
      "handler": {
        "export": "ValidateJsonSchemaInbound",
        "module": "$import(@zuplo/runtime)",
        "options": {
          "validator": "$import(./schemas/validate-vehicle-plate.json)"
        }
      },
      "name": "validate-json-schema-inbound",
      "policyType": "validate-json-schema-inbound"
    }
  ]
}