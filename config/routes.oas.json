      "InsertOrderObject": {
        "type": "object",
        "title": "Order Insert Schema",
        "required": [
          "terms_and_privacy_accepted",
          "order_has_been_paid",
          "email",
          "cars",
          "products"
        ],
        "additionalProperties": false,
        "properties": {
          "terms_and_privacy_accepted": {
            "type": "boolean",
            "description": "Must be `true` to indicate that terms and privacy are accepted.",
            "enum": [
              "true"
            ],
            "examples": [
              true
            ]
          },
          "order_has_been_paid": {
            "type": "boolean",
            "description": "Must be `true` to indicate that the order has been paid.",
            "enum": [
              "true"
            ],
            "examples": [
              true
            ]
          },
          "email": {
            "type": "string",
            "format": "email",
            "description": "User's email for sending receipts or contacting in case of data errors.",
            "examples": [
              "user@gmail.com"
            ]
          },
          "cars": {
            "type": "array",
            "description": "Array of cars with car plate and registration country.",
            "items": {
              "type": "object",
              "required": [
                "plate",
                "country"
              ],
              "properties": {
                "plate": {
                  "type": "string",
                  "description": "Carplate number (vehicle plate)\n\n **Important!** \n Only numbers and Latin characters \n\n **Exception** \n German and Austrian license plates where the car number \n must contain a minus `-` sign. \n\n [More about car plate templates](https://vignette-api-main-982aec0.d2.zuplo.dev/docs/routes/patterns)",
                  "example": "AA4544AA"
                },
                "country": {
                  "type": "string",
                  "description": "Car registration country code. Use two lowercase(!) letter code format like `at`, `cz`, `pl`, `ua`... \n\n [More about country codes. IMPORTANT to read](https://vignette-api-main-982aec0.d2.zuplo.dev/docs/routes/countries)",
                  "example": "ua"
                }
              }
            }
          },
          "products": {
            "type": "array",
            "description": "Array of vignettes with custom id, name, period, and start date.",
            "items": {
              "type": "object",
              "required": [
                "name",
                "start_from",
                "period"
              ],
              "properties": {
                "custom_id": {
                  "type": "string",
                  "description": "Order id in Partner system",
                  "example": "partnername-000001"
                },
                "name": {
                  "type": "string",
                  "description": "Country of vignette. \n\n To check options - use endpoint `/public/products -> name`",
                  "example": "hu-vignette-d1"
                },
                "start_from": {
                  "type": "integer",
                  "format": "int64",
                  "description": "Start date of vignette in UNIX time",
                  "example": 1690711200
                },
                "period": {
                  "type": "integer",
                  "description": "Period of vignette. Available periods depend on the country. \n Possible options `7`, `10`, `30`, `60`, `90`, `180`, `365`. \n\n To check options - use endpoint `/public/products -> price`",
                  "example": 30
                }
              }
            }
          },
          "interchange_fee": {
            "type": "object",
            "required": [
              "amount",
              "currency"
            ],
            "properties": {
              "amount": {
                "type": "number",
                "description": "Gateway fee",
                "examples": [
                  100,
                  3.14
                ]
              },
              "currency": {
                "type": "string",
                "description": "The fee currency. Available in `EUR`, `UAH`, `PLN`.",
                "examples": [
                  "UAH",
                  "EUR"
                ]
              }
            }
          }
        },
        "examples": [
          {
            "terms_and_privacy_accepted": true,
            "order_has_been_paid": true,
            "email": "user@example.com",
            "cars": [
              {
                "plate": "AA4544AA",
                "country": "ua"
              }
            ],
            "products": [
              {
                "custom_id": "partnername-000001",
                "name": "hu-vignette-d1",
                "start_from": 1690711200,
                "period": 30
              }
            ],
            "interchange_fee": {
              "amount": 0.33,
              "currency": "EUR"
            }
          }
        ]
      },
