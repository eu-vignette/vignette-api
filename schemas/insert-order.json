{
  "type": "object",
  "default": {},
  "title": "Root Schema",
  "additionalProperties": false,
  "required": [
    "terms_and_privacy_accepted",
    "order_has_been_paid",
    "email",
    "cars",
    "products"
  ],
  "properties": {
    "terms_and_privacy_accepted": {
      "type": "boolean",
      "default": false,
      "title": "The terms_and_privacy_accepted Schema",
      "examples": [true]
    },
    "order_has_been_paid": {
      "type": "boolean",
      "default": false,
      "title": "The order_has_been_paid Schema",
      "examples": [true]
    },
    "email": {
      "type": "string",
      "default": "",
      "title": "The partner user email. Must contain the user's real email",
      "examples": ["test@example.com"]
    },
    "custom_id": {
      "type": "string",
      "default": "",
      "title": "Partner id",
      "examples": ["00asd4123"]
    },
    "cars": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "plate": {
            "type": "string",
            "default": "",
            "title": "The plate Schema",
            "examples": ["LJ000ID"]
          },
          "vin_code": {
            "type": "string",
            "default": "",
            "title": "The vin code Schema",
            "examples": ["VWT045FD9812VB341"]
          },
          "country": {
            "type": "string",
            "default": "",
            "title": "The country Schema",
            "examples": ["si"]
          }
        }
      }
    },
    "products": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "custom_id": {
            "type": "string",
            "default": "",
            "title": "Partner custom id",
            "examples": ["partner-00001"]
          },
          "name": {
            "type": "string",
            "default": "",
            "title": "Name of vignette",
            "examples": ["vignette-si-2a"]
          },
          "start_date": {
            "type": "integer",
            "default": 0,
            "title": "Start date of period",
            "examples": [1690711200]
          },
          "start_from": {
            "type": "integer",
            "default": 0,
            "title": "Start date of period",
            "examples": [1690711200]
          },
          "period": {
            "type": ["integer", "string"],
            "default": 0,
            "title": "Days amount in period",
            "examples": [30]
          }
        }
      }
    },
    "interchange_fee":{
      "type": "object",
      "properties": {
        "amount": {
          "type": "number",
          "default": "",
          "title": "The partner payment fee",
          "examples": [100, 3.24]
        },
        "currency": {
          "type": "string",
          "default": "UAH",
          "title": "The fee currency",
          "examples": ["UAH", "EUR"]
        }
      }
    },
    "vat_fee":{
      "type": "object",
      "properties": {
        "amount": {
          "type": "number",
          "default": "",
          "title": "The partner vat fee",
          "examples": [100, 3.24]
        },
        "currency": {
          "type": "string",
          "default": "UAH",
          "title": "The fee currency",
          "examples": ["UAH", "EUR"]
        }
      }
    }
  },
  "examples": [
    {
      "terms_and_privacy_accepted": true,
      "order_has_been_paid": true,
      "email": "test@example.com",
      "cars": [
        {
          "plate": "AA4544AA",
          "country": "ua"
        }
      ],
      "products": [
        {
          "custom_id": "partnername-000001",
          "name": "vignette-si-2a",
          "start_from": "WILL BE DEPRECATED. Use start_date",
          "start_date": 1690711200,
          "period": 30
        }
      ],
      "interchange_fee": {
        "amount": 0.33,
        "currency": "EUR"
      }
    }
  ]
}
